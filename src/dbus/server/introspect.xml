    <!-- RadioSpinelMetrics: The radio spinel metrics
    <literallayout>
        struct {
          uint32_t mRcpTimeoutCount;         // The number of RCP timeouts.
          uint32_t mRcpUnexpectedResetCount; // The number of RCP unexcepted resets.
